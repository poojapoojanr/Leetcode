class Solution:
    def getRow(self, rowIndex: int) -> List[int]:
        triangle = []
        numRows = rowIndex 
        for i in range(numRows + 1):
            row = [1] * (i + 1)  # Initialize row with 1s
            if i > 1:
                for j in range(1, i):
                    # Calculate middle elements by summing elements from the previous row
                    row[j] = triangle[i-1][j-1] + triangle[i-1][j]
            triangle.append(row)
        res = triangle.pop()
        return res
