class Solution:
    def countAndSay(self, n: int) -> str:
        res = "1"
        for i in range(1,n):
            newdata = ""
            count = 1
            cur = res[0]
            for j in res[1:]:
                if j == cur:
                    count +=1
                else:
                    newdata = newdata + str(count) + cur
                    cur = j
                    count = 1
            res = newdata + str(count) + cur
        return res

        
